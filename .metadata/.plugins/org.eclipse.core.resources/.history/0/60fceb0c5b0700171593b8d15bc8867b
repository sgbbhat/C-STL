#include <iostream>
using namespace std;

struct node
{
	int Key;
	node * next;
};

class Queue
{
private:
	node * front;
	node * back;

public:
	Queue();
	void push(int key);
	void pop();
	void PrintQueue();
};

Queue::Queue()
{
	front = NULL;
	back = NULL;
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
