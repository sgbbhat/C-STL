#include <iostream>
using namespace std;

struct node
{
	int Key;
	node * next;
};

class Stack
{
private:
	node * front;
	node * rear;
	node * curr;

public:
	Stack();
	void push(int key);
	void pop();
	void PrintStack();
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
