#include <iostream>
#include <map>
#include <string>
using namespace std;

int main() {
	map<string,int> quiz;
	quiz["Paul"] = 20;
	quiz["Alex"] = 30;
	quiz["Steve"] = 40;
	quiz["Max"] = 50;
	return 0;
}
