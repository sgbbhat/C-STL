#include <iostream>
#include <string>
using namespace std;

class Account
{
private:
	int number;
	string name;

public:
	void PrintDetails()
	{
		cout<<"Printing Account"<<endl;
	}
};

class savings:public Account
{
public:
	void PrintDetails()
	{
		cout<<"Printing Account"<<endl;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
