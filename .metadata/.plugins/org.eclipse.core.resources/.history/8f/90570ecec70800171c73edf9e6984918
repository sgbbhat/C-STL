#include <iostream>
using namespace std;

struct node
{
	int Key;
	node * next;
};

class List
{
private:
	node * headPtr;
	node * currPtr;
	node * tempPtr;

public:
	List();
	void AddNode(int Key);
	void DeleteNode(int Key);
	void PrintList();
};



int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
