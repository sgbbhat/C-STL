#include <iostream>
#include <string>
#include <list>
#include <map>


using namespace std;

int main() {
	list<int> test;
	list<int>::iterator it;
	test.insert(it,10);

	map<int, string> test1;
	test1.insert(1, "yes00");
	test1.find(1);
	string Test1 = "Hello";
	string Test2 = "World";
	return 0;
}
