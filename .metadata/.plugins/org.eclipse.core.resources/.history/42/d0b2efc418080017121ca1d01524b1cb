#include <iostream>
#include <map>
using namespace std;

bool IsUnique(string S)
{
	map<char,int> CharSet(26);
	map<char,int>::iterator it;
	map['a'] = 0;
	map['b'] = 0;
	map['c'] = 0;
	map['d'] = 0;
	map['e'] = 0;
	map['f'] = 0;
	map['g'] = 0;
	map['h'] = 0;

	for(int i=0; i<S.length(); i++)
	{
		it = CharSet.find(S[i]);

		if(CharSet[it] < 1)
		{
			CharSet[it] = CharSet[it] + 1;
		}
	}
}

int main() {

	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
