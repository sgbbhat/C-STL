#include <iostream>
#include <vector>
using namespace std;

int main() {
	vector<int> intVec(20);

	for(int i=0; i<20; i++)
	{
		intVec[i] = i;
	}

	for(int i=0; i<intVec.size(); i++)
	{
		cout<<intVec.at(i) <<endl;
	}

	for(int i=0; i<5; i++)
	{
		intVec.push_back(i);
	}

	return 0;
}
