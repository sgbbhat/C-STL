#include <iostream>
#include <list>
using namespace std;

int main() {
	int array1[] {10, 40, 30, 20};
	int array2[] {99, 25, 2, 99};

	list<int> lis1(array1);
	list<int> list2(array2);

	list<int>::iterator it_list1;
	list<int>::iterator it_list2;

 	return 0;
}
