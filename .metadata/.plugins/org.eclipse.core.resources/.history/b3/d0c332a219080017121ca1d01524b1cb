#include <iostream>
#include <map>
using namespace std;

bool IsUnique(string S)
{
	map<char,int> CharSet;
	map<char,int>::iterator it;
	CharSet['a'] = 0;
	CharSet['b'] = 0;
	CharSet['c'] = 0;
	CharSet['d'] = 0;
	CharSet['e'] = 0;
	CharSet['f'] = 0;
	CharSet['g'] = 0;
	CharSet['h'] = 0;

	bool unique ;
	for(unsigned i=0; i<S.length(); i++)
	{
		it = CharSet.find(S[i]);
		if(it->second < 1)
		{
			it->second = it->second + 1;
		}
		else
		{
			unique = false;
		}
	}

	return unique;
}

int main() {

	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
