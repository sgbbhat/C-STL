#include <iostream>
#include <vector>
using namespace std;

int main() {
	vector<int> intVec(20);

	for(int i=0; i<20; i++)
	{
		intVec[i] = i;
	}
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
