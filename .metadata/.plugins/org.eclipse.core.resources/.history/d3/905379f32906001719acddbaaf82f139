// Description : Hello World in C++, Ansi-style
//============================================================================

#include <iostream>
#include <string>

using namespace std;


int main() {

	string s1 = "Hello World";
	string::iterator it;

	for(it = s1.begin(); it != s1.end(); it++)
	{
		cout<<*it<<endl;
	}


	for(it = s1.rbegin(); it != s1.rend(); it++)
	{
		cout<<*it<<endl;
	}

	return 0;
}
